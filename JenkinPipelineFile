pipeline {
    agent any

    stages {
        stage('From SCM') {
            steps {
                echo 'Directed from SCM'
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
       
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}
